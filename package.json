{
  "name": "electron-umi-dva-boilerplate-template",
  "version": "0.0.0",
  "description": "electron-umi-dva-boilerplate-template",
  "private": false,
  "main": "app/main.js",
  "build": {
    "appId": "com.electron.electron-umi-dva-boilerplate-template",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "scripts": {
    "app-dev": "concurrently \"npm run start\" \"wait-on http://localhost:8000 && npm run electron-dev\"",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "electron-prod": "cross-env NODE_ENV=production electron .",
    "start": "umi dev",
    "build": "umi build",
    "clean": "rimraf dist",
    "pack:mac": "npm run clean && npm run build && npm run private:build:mac",
    "pack:win": "npm run clean && npm run build && npm run private:build:win",
    "private:build:all": "electron-builder -mwl",
    "private:build:mac": "electron-builder --mac",
    "private:build:win": "electron-builder -c.extraMetadata.main=app/main.js --win --x64",
    "private:build:linux": "electron-builder --linux",
    "test": "umi test",
    "lint": "eslint --ext .js src mock tests",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "antd": "^3.19.2",
    "dva": "^2.6.0-beta.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-reload": "^1.4.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "react-transition-group": "^4.1.1",
    "rimraf": "^2.6.3",
    "umi": "^2.7.0",
    "umi-plugin-react": "^1.8.0",
    "wait-on": "^3.2.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
